<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	 xmlns="http://www.zimmer.com/tibco/schemas/ZBS/XSD/ExceptionHandler/v1/ExceptionMessageSchema.xsd"
	 targetNamespace="http://www.zimmer.com/tibco/schemas/ZBS/XSD/ExceptionHandler/v1/ExceptionMessageSchema.xsd"
	 elementFormDefault="qualified"
	 attributeFormDefault="unqualified">
	<xs:element name="ExceptionMessage">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Application" type="xs:string"/>
				<xs:element name="Category" type="xs:string" minOccurs="0"/>
				<xs:element name="Severity" type="xs:string"/>
				<xs:element name="ExceptionDescription" type="xs:string"/>
				<xs:element name="ExceptionDetailsText" type="xs:string" minOccurs="0"/>
				<xs:element name="ExceptionType" type="xs:string"/>
				<xs:element name="ExceptionCode" type="xs:string"/>
				<xs:element name="PayLoad" type="xs:string" minOccurs="0"/>
				<xs:element name="ProcessId" type="xs:string"/>
				<xs:element name="ProcessName" type="xs:string"/>
				<xs:element name="EngineName" type="xs:string"/>
				<xs:element name="CreateTimestamp" type="xs:dateTime"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>